{
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "2018"
  },
  "env": {
    "browser": true,
    "es6": true,
    "amd": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": [
      "off"
    ]
  }
}
